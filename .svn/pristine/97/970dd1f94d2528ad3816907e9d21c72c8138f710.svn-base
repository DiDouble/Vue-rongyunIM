body{
	font-weight:normal;
	font-style:normal;
	line-height:normal;
	-webkit-box-sizing:border-box;
	box-sizing:border-box;
	font-family:PingFangSC-Medium, sans-serif;
}
html{
	background: #f5f5f5;
	height: 100%;
}

a{
	text-decoration:none !important;
}
a:active,a:link,a:visited,a:hover{
        text-decoration: none !important;
}
ul{
	list-style:none;
}
img{
	border:none;
}
*{
	margin:0;
	padding:0;
	-webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
#loadingImg{
	display: block;
	height:0.86rem;
	width: 0.86rem;
	position: fixed;
	left: 45%;
	top: 40%;
}
.clearfix{
	min-height: 0px;
	height:auto!important;
	height: 0px;
}
.clearfix:after{
	content: '.';
	display: block;
	height: 0;
	clear: both;
	overflow: hidden;
	visibility: hidden;
}

a,button,input,textarea,div,span,p,h1,ul,li{
	border: none;
	outline:none;
	background: none;
	/*-webkit-tap-highlight-color:rgba(255,0,0,0);*/
}
input:focus,textarea:focus{
	outline:none;
	/*-webkit-tap-highlight-color:rgba(255,0,0,0);*/
}  
::-webkit-input-placeholder { /* WebKit browsers */
	color:#aaa;
}
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
	color:#aaa;
}
::-moz-placeholder { /* Mozilla Firefox 19+ */
	color:#aaa;
}
:-ms-input-placeholder { /*Internet Explorer 10+ */
	color:#aaa;
}